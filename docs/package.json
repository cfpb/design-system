{
  "name": "",
  "description": "A boilerplate for creating Netlify CMS widgets.",
  "keywords": [
    "netlify",
    "netlify-cms",
    "cms",
    "widget",
    "starter",
    "boilerplate"
  ],
  "version": "0.0.1",
  "homepage": "https://github.com/netlify/",
  "license": "MIT",
  "main": "dist/main.js",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-export-extensions": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "netlify-cms": "^2.9.1",
    "parallelshell": "3.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-serve": "^0.3.1"
  },
  "scripts": {
    "start": "parallelshell \"bundle exec jekyll serve --watch --host=localhost\" \"yarn run watch\" \"yarn run watch-css\"",
    "compile-css": "lessc --include-path=../src/packages/:../src/node_modules/normalize-css/normalize-css/ assets/css/main.less dist/css/main.css",
    "watch-css": "less-watch-compiler --less-args include-path=../src/packages/:../src/node_modules/normalize-css/normalize-css/ assets/css dist/css main.less",
    "watch": "webpack --watch",
    "build": "bundle exec jekyll build && NODE_ENV=production webpack && yarn run compile-css",
    "build-preview": "bundle exec jekyll build && webpack && yarn run compile-css",
    "install": "yarn --cwd ../src"
  },
  "dependencies": {
    "escape-html": "^1.0.3",
    "fetch-suspense": "^1.1.0",
    "ftdomdelegate": "^3.0.0",
    "govuk-frontend": "^2.13.0",
    "less-watch-compiler": "^1.13.0",
    "marked": "^0.6.2",
    "normalize-css": "^2.3.1",
    "normalize-legacy-addon": "^0.1.0",
    "react": "^16.8.6",
    "react-async": "^6.2.0",
    "react-dom": "^16.8.6",
    "react-liquid": "^1.2.0"
  }
}
