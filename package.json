{
  "name": "cfpb-design-system",
  "version": "0.7.1",
  "private": true,
  "description": "CFPB's UI framework",
  "keywords": [
    "design-system"
  ],
  "bugs": {
    "url": "https://github.com/cfpb/design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cfpb/design-system.git"
  },
  "license": "SEE LICENSE IN TERMS.md",
  "author": {
    "name": "Consumer Financial Protection Bureau",
    "email": "tech@cfpb.gov",
    "url": "https://cfpb.github.io/"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "a11y": "lhci autorun",
    "build": "yarn run compile-dev && bundle exec jekyll build",
    "build-netlify": "yarn && yarn run compile-prod && bundle exec jekyll build",
    "changelog": "./scripts/generate-changelog.sh",
    "compile-prod": "webpack --config webpack.config.docs.js --mode=production",
    "compile-dev": "webpack --config webpack.config.docs.js --mode=development",
    "copy-icons": "cp -r packages/cfpb-icons/src/icons docs/assets/",
    "design-system-link": "lerna exec -- yarn link",
    "design-system-unlink": "lerna exec -- yarn unlink",
    "install": "yarn run copy-icons && bundle install",
    "process-icon-svgs": "svgo -o packages/cfpb-icons/src/cfpb-icons --enable=addClassesToSVGElement --enable=removeStyleElement --enable=removeAttrs --config='{ \"plugins\": [ { \"addClassesToSVGElement\": { \"className\": \"cf-icon-svg\" } }, { \"removeAttrs\": { \"attrs\": [ \"fill\", \"path:class\", \"circle:class\" ] } }, { \"cleanupIDs\": { \"force\": \"true\" } } ] }'",
    "release": "yarn run build && lerna publish",
    "start": "yarn run compile-dev && concurrently --kill-others \"yarn serve-netlify\" \"yarn serve-jekyll\" \"yarn run compile-dev --watch\"",
    "serve-html": "http-server docs/_site",
    "serve-jekyll": "bundle exec jekyll serve --watch --profile --host=localhost --port=4000",
    "serve-netlify": "netlify-cms-proxy-server",
    "test": "gulp test",
    "test:browser": "wdio run test/browser/webdriver.conf.js",
    "test:browser:cloud": ": ${SAUCE_USERNAME?\"You need to define SAUCE_USERNAME and SAUCE_ACCESS_KEY env variables \"} && wdio run test/browser/webdriver-sauce.conf.js",
    "version": "yarn run changelog && git add CHANGELOG.md"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead, ie >= 11",
  "changelog": {
    "labels": {
      "lerna-changelog/breaking": "Breaking Change",
      "lerna-changelog/enhancement": "Enhancement",
      "lerna-changelog/bug": "Bug Fix",
      "lerna-changelog/documentation": "Documentation",
      "lerna-changelog/internal": "Internal"
    }
  },
  "dependencies": {
    "anchor-js": "4.3.0",
    "core-js": "3.8.3",
    "ftdomdelegate": "4.0.6",
    "govuk-frontend": "3.10.2",
    "html-entities": "2.1.0",
    "lunr": "2.3.9",
    "marked": "2.0.0",
    "netlify-cms": "2.10.88",
    "normalize-css": "^2.3.1",
    "normalize-legacy-addon": "^0.1.0",
    "react": "17.0.1",
    "react-liquid": "2.0.1",
    "regenerator-runtime": "0.13.7",
    "slugify": "1.4.6"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/plugin-syntax-export-extensions": "^7.0.0-beta.32",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@lhci/cli": "0.7.0",
    "@wdio/cli": "6.12.1",
    "@wdio/local-runner": "6.12.1",
    "@wdio/mocha-framework": "^6.11.0",
    "@wdio/sauce-service": "6.12.1",
    "@wdio/spec-reporter": "^6.11.0",
    "@wdio/sync": "6.11.0",
    "autoprefixer": "10.2.4",
    "babel-loader": "8.2.2",
    "chromedriver": "88.0.0",
    "concurrently": "5.3.0",
    "css-loader": "5.0.1",
    "cssnano": "^4.1.10",
    "escape-html": "^1.0.3",
    "fancy-log": "^1.3.2",
    "finalhandler": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "6.0.0",
    "gulp-ignore": "3.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-stylelint": "13.0.0",
    "html-loader": "1.3.2",
    "http-server": "^0.12.3",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lerna": "3.22.1",
    "lerna-changelog": "1.0.1",
    "less-loader": "8.0.0",
    "log-symbols": "4.0.0",
    "merge-stream": "2.0.0",
    "mini-css-extract-plugin": "1.3.5",
    "minimist": "1.2.5",
    "netlify-cms-proxy-server": "1.3.9",
    "postcss": "8.2.4",
    "postcss-loader": "5.0.0",
    "require-dir": "^1.2.0",
    "serve-static": "^1.13.1",
    "style-loader": "2.0.0",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "svg-inline-loader": "0.8.2",
    "svgo": "1.3.2",
    "terser-webpack-plugin": "5.1.1",
    "through2": "4.0.2",
    "vinyl-named": "^1.1.0",
    "wcag": "^0.3.0",
    "wdio-chromedriver-service": "6.0.4",
    "webpack": "5.20.2",
    "webpack-cli": "4.5.0",
    "webpack-stream": "6.1.2"
  }
}
