{
  "name": "cfpb-design-system",
  "private": true,
  "description": "CFPB's UI framework",
  "keywords": [
    "design-system"
  ],
  "bugs": {
    "url": "https://github.com/cfpb/design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cfpb/design-system.git"
  },
  "license": "SEE LICENSE IN TERMS.md",
  "author": {
    "name": "Consumer Financial Protection Bureau",
    "email": "tech@cfpb.gov",
    "url": "https://cfpb.github.io/"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "a11y": "lhci autorun",
    "build": "yarn compile-dev && bundle exec jekyll build",
    "build-netlify": "yarn install && yarn after-install && yarn compile-prod && bundle exec jekyll build",
    "build-packages": "webpack --config webpack.config.packages.js --mode=production",
    "changelog": "./scripts/generate-changelog.sh",
    "compile-dev": "webpack --config webpack.config.docs.js --mode=development",
    "compile-prod": "webpack --config webpack.config.docs.js --mode=production",
    "copy-assets": "./scripts/fonts.sh && cp -r packages/cfpb-icons/src/icons docs/_includes/",
    "design-system-link": "lerna exec -- yarn link",
    "design-system-unlink": "lerna exec -- yarn unlink",
    "after-install": "yarn copy-assets && bundle install",
    "lint": "scripts/lint.sh",
    "process-icon-svgs": "svgo -f packages/cfpb-icons/src/icons --config=svgo.config.js",
    "pre-release": "./scripts/pre-release.sh",
    "release": "lerna publish",
    "serve-html": "http-server docs/_site",
    "serve-jekyll": "bundle exec jekyll serve --watch --profile --host=localhost --port=4000",
    "serve-netlify": "netlify-cms-proxy-server",
    "start": "yarn compile-dev && concurrently --kill-others \"yarn serve-netlify\" \"yarn serve-jekyll\" \"yarn compile-dev --watch\"",
    "jest": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "test:browser": "yarn cypress run",
    "test:browser:cloud": ": ${SAUCE_USERNAME?\"You need to define SAUCE_USERNAME and SAUCE_ACCESS_KEY env variables \"} && node_modules/saucectl/bin/saucectl run"
  },
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead, ie >= 11",
  "dependencies": {
    "anchor-js": "5.0.0",
    "html-entities": "2.4.0",
    "lunr": "2.3.9",
    "marked": "4.2.12",
    "netlify-cms": "2.10.192",
    "normalize-css": "2.3.1",
    "react": "18.2.0",
    "react-is": "18.2.0",
    "react-liquid": "2.0.1",
    "slugify": "1.6.6"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/preset-env": "7.23.5",
    "@babel/preset-react": "7.23.3",
    "@lhci/cli": "0.13.0",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.2",
    "css-loader": "6.8.1",
    "cssnano": "6.0.1",
    "cypress": "13.6.3",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "filemanager-webpack-plugin": "8.0.0",
    "html-loader": "4.2.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lerna": "8.1.2",
    "lerna-changelog": "2.2.0",
    "less": "4.2.0",
    "less-loader": "11.1.3",
    "mini-css-extract-plugin": "2.7.6",
    "netlify-cms-proxy-server": "1.3.24",
    "postcss": "8.4.32",
    "postcss-less": "6.0.0",
    "postcss-loader": "7.3.3",
    "prettier": "3.1.0",
    "prop-types": "15.8.1",
    "saucectl": "0.170.0",
    "stylelint": "15.11.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-less": "2.0.0",
    "svg-inline-loader": "0.8.2",
    "svgo": "3.0.5",
    "terser-webpack-plugin": "5.3.9",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "changelog": {
    "labels": {
      "lerna-changelog/breaking": "Breaking Change",
      "lerna-changelog/enhancement": "Enhancement",
      "lerna-changelog/bug": "Bug Fix",
      "lerna-changelog/documentation": "Documentation",
      "lerna-changelog/internal": "Internal"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "type": "module"
}
